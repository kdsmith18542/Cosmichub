{
    "name": "cosmichub/online",
    "description": "CosmicHub.Online - Astrology Reports Platform",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "google-gemini-php/client": "^2.3",
        "nyholm/psr7": "^1.8",
        "psr/http-client": "^1.0",
        "symfony/http-client": "^7.3",
        "tecnickcom/tcpdf": "^6.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
